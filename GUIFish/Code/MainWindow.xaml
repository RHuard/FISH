<Window x:Class="GUIFish.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Fish Sticks" Height="314" Width="401">
    <Grid Margin="0,0,2,2">
        <Button x:Name="Make_TPS_Buttun" Content="Make TPS" HorizontalAlignment="Left" Margin="10,32,0,0" VerticalAlignment="Top" Width="140" Height="79" Click="Make_TPS_Buttun_Click"/>
        <Button x:Name="Tps_To_Cvs_Button" Content="TPS to CSV" HorizontalAlignment="Left" Margin="238,32,0,0" VerticalAlignment="Top" Width="140" Height="79" RenderTransformOrigin="0.5,0.5" Click="Tps_To_Cvs_Button_Click"/>
        <Button x:Name="Close_Button" Content="Close" HorizontalAlignment="Left" Margin="280,230,0,0" VerticalAlignment="Top" Width="98" Height="34" Click="Close_Button_Click"/>
        <Button x:Name="Help_Button" Content="Help" HorizontalAlignment="Left" Height="34" Margin="170,230,0,0" VerticalAlignment="Top" Width="89" Click="Help_Button_Click"/>

    </Grid>
</Window>
